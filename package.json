{
  "name": "nekocap",
  "version": "0.3.3",
  "description": "An awesome caption viewing and editing browser extension",
  "private": true,
  "scripts": {
    "build": "concurrently \"npm:build:ext\" \"npm:build:web\"",
    "build:ext": "webpack --config webpack.extension.js --mode=production",
    "build:web": "webpack --config webpack.web.js --mode=production",
    "dev": "concurrently \"npm:dev:ext\" \"npm:dev:web\"",
    "dev:ext": "webpack --config webpack.extension.js --mode=development",
    "dev:web": "webpack --config webpack.web.js --mode=development",
    "watch": "concurrently \"npm:watch:ext\" \"npm:watch:web\"",
    "watch:ext": "webpack-dev-server --config webpack.extension.js --mode=development",
    "watch:web": "webpack-dev-server --config webpack.web.js --mode=development",
    "analyze:ext": "webpack --config webpack.extension.js --mode=production --analyze=true",
    "analyze:web": "webpack --config webpack.web.js --mode=production --analyze=true",
    "lint": "eslint --ext .ts,.tsx ./src",
    "lint:fix": "eslint --ext .ts,.tsx --fix ./src"
  },
  "author": "nopol10",
  "license": "gpl-3.0",
